{
  "name": "ember-collapsible-panel",
  "version": "4.0.0",
  "description": "An unopinionated, zero-dependency panel and accordion",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "TEDconf and contributors",
  "contributors": [
    {
      "email": "alonzorz@gmail.com",
      "name": "Alon Bukai"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adopted-ember-addons/ember-collapsible-panel.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.3.1",
    "ember-cli-version-checker": "^2.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^3.3.7",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.18.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "0.0.6",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "7.1.7",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "^1.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-ted-docs": "Alonski/ember-ted-docs",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.4.0",
    "eslint-plugin-node": "^11.1.0",
    "liquid-fire": "0.29.5",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "bugs": {
    "url": "https://github.com/adopted-ember-addons/ember-collapsible-panel/issues"
  },
  "homepage": "https://adopted-ember-addons.github.io/ember-collapsible-panel/",
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adopted-ember-addons.github.io/ember-collapsible-panel/"
  }
}
